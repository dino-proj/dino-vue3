{
  "name": "@dino-dev/vue3-core",
  "type": "module",
  "version": "1.0.3",
  "description": "dino dev framework javascript sdk base",
  "repository": "https://github.com/dino-proj/dino-vue3",
  "author": "Cody Lu",
  "license": "Apache-2.0",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint --fix ./dist --ext .js,.ts",
    "clean": "rimraf dist",
    "build": "yarn run clean && yarn vite build",
    "release": "yarn run build && npm publish"
  },
  "peerDependencies": {
    "dayjs": ">=1.10.7",
    "lodash-es": ">=4.17.15",
    "mitt": ">=3.0.0",
    "nprogress": ">=1.0.0-1",
    "pinia": ">=2.0.4",
    "qs": ">=6.10.3",
    "web-storage-cache": ">=1.1.1"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "is-ip": "^3.1.0",
    "lodash-es": ">=4.17.15"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@rollup/plugin-alias": "^3.1.4",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/eslint": "^8.44.9",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.4",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^6.14.0",
    "axios": ">=0.23.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^6.14.0",
    "prettier": "^2.4.1",
    "rimraf": "^5.0.5",
    "rollup": "^2.54.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.6.4"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "engines": {
    "node": ">=18.12.0"
  }
}
